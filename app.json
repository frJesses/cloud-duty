{
  "expo": {
    "name": "云值守",
    "slug": "cloud-duty-www",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "cloudduty",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "需要使用相机来拍摄照片和扫描二维码"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.wwlbody.clouddutywww",
      "softwareKeyboardLayoutMode": "pan",
      "permissions": ["CAMERA"]
    },
    "androidNavigationBar": {
      "backgroundColor": "#FFFFFF",
      "barStyle": "dark-content",
      "visible": "visible"
    },
    "androidStatusBar": {
      "backgroundColor": "#FFFFFF",
      "barStyle": "dark-content",
      "translucent": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/adaptive-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/adaptive-icon.png",
            "imageWidth": 200,
            "resizeMode": "contain",
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-barcode-scanner",
      [
        "expo-camera",
        {
          "cameraPermission": "允许使用相机来拍照和扫描二维码"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "48687dcf-3dac-43fa-b82c-4308e1536684"
      }
    }
  }
}
